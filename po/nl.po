#: panel-plugin/systemload.cc:233
#, c-format
msgid "System Load: %ld%%"
msgstr "Systeembelasting: %ld%%"

#: panel-plugin/systemload.cc:240
#, c-format
msgid "Memory: %ldMB of %ldMB used"
msgstr "Geheugen: %ldMB van %ldMB in gebruik"

